<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.encore.outpick_backend.Login.mapper">

    <select id="login"
        parameterMap="com.encore.outpick_backend.Login.domain.LoginDTO"
        resultType="com.encore.outpick_backend.Login.domain.LoginDTO">

        SELECT 
            employee_number, password
        FROM 
            employee
        WHERE 
            employee_number = #{employee_number}

    </select>

</mapper>